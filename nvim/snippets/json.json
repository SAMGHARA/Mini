{
    "snippet": {
        "prefix": "snippet",
        "body": [
            "\"$1\": {",
            "\t\"prefix\": \"$2\",",
            "\t\"body\": [$3],",
            "\t\"description\": \"$4\"",
            "}"
        ],
        "description": "VSCode Snippet"
    },
    "language": {
        "prefix": "language",
        "body": [
            "{",
            "\t\"language\": \"${1:language}\",",
            "\t\"path\": \"./${1:language}.json\"",
            "}"
        ]
    }
}
