{
    "mycout": {
        "prefix": "mycout",
        "body": [
            "std::cout << $1 << std::endl;"
        ],
        "description": "cpp std::cout Snippet"
    },
    "main": {
        "prefix": "main",
        "body": [
            "int main(int argc, char *argv[]) {",
